{
  "name": "student-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/student-e2e/cypress",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/student-e2e/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/student-e2e",
        "tsConfig": "apps/student-e2e/tsconfig.json",
        "entryFile": "apps/student-e2e/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel"
      }
    },
    "cypress-open": {
      "executor": "@nx/cypress:cypress",
      "defaultConfiguration": "local",
      "options": {
        "cypressConfig": "apps/student-e2e/cypress.config.ts",
        "testingType": "e2e"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "student:serve:development",
          "watch": false
        },
        "local": {
          "watch": true
        }
      }
    }
  }
}
